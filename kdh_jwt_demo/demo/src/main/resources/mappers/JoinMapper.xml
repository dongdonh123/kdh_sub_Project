<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.demo.mapper.JoinMapper">

    <insert id="joinProcess"  parameterType="com.example.demo.model.UserDTO">
        INSERT INTO kdh.user_mgt
        (
        user_id,
        user_passwd,
        user_role
        )
        VALUES
        (
        #{user_id},
        #{user_passwd},
        #{user_role}
        );
    </insert>


</mapper>